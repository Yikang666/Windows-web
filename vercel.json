{
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "status": 404,
            "dest": "/error/404.html"
        },
        {
            "src": "/win7/(.*)",
            "dest": "/Windows 7/(.*)"
        },
        {
            "src": "/win10/(.*)",
            "dest": "/Windows 10/(.*)"
        }
    ]
}