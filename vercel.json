{
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "status": 404,
            "dest": "/error/404.html"
        }
    ],
    "rewrites": [
        {
            "source": "/win7/(.*)",
            "destination": "/Windows 7/"
        },
        {
            "source": "/win10/(.*)",
            "destination": "/Windows 10/"
        }
    ]
}